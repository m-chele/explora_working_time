






.hfpf_loading_alt_container {
}
.hfpf_loading_alt_portlet{
  position:relative;
  width:220px;
  min-width:220px;
  height:30px;
  background-color:transparent;
}
.hfpf_loading_alt_portlet > .image0_ctrl {
  box-sizing:border-box;
  z-index:1;
  position:absolute;
  display:inline-block;
  top:1px;
  left:13px;
  width:22px;
  height:22px;
}
.hfpf_loading_alt_portlet > .image0_ctrl {
}
.hfpf_loading_alt_portlet > .image0_ctrl > img{
  width:100%;
  vertical-align:top;
  height:22px;
}
.hfpf_loading_alt_portlet > .label1_ctrl {
  box-sizing:border-box;
  z-index:1;
  position:absolute;
  display:inline-block;
  top:5px;
  left:45px;
  width:123px;
  height:20px;
}
.hfpf_loading_alt_portlet > .label1_ctrl {
  height:auto;
  min-height:20px;
}
.hfpf_loading_alt_portlet > .label1_ctrl {
  overflow:hidden;
  font-family:Verdana;
  font-size:10px;
  font-weight:normal;
  color:#666666;
  text-align:left;
  background-color:transparent;
}

